{
    "name": "truelab/kotti-security-bundle",
    "description": "A symfony2 bundle that provides \"security\" integration, admin features for kotti frontend.",
    "license": "MIT",
    "authors": [
        {
            "name": "Ruben Barilani",
            "email": "ruben.barilani.dev@gmail.com"
        }
    ],
    "autoload" : {
        "psr-4": {"Truelab\\KottiSecurityBundle\\" : "./"}
    },
    "require": {
        "php": ">=5.4.0",
        "symfony/monolog-bundle": "~2.4",
        "doctrine/doctrine-bundle": "~1.3",
        "doctrine/orm": "~2.3",
        "sensio/framework-extra-bundle": "~3.0",
        "truelab/kotti-model-bundle": "~0.4"
    },
    "require-dev": {
        "symfony/symfony": "2.6.*",
        "phpunit/phpunit": "4.6",
        "matthiasnoback/symfony-config-test" : "~0.4",
        "matthiasnoback/symfony-dependency-injection-test" : "~0.7"
    },
    "scripts" : {
        "test" : "vendor/bin/phpunit"
    },
    "repositories" : [
        { "type":"git", "url":"https://github.com/truelab/kotti-model-bundle.git" }
    ]
}
